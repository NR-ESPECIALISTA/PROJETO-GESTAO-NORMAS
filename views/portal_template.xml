                                                </div>
                                                <div class="col-sm-9">
                                                    <p class="text-muted mb-0">
                                                        <span
                                                            t-field="equipamentos.equip_relacional_ids" />
                                                    </p>
                                                </div>
                                            </div>
                                        </hr>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- chatter -->
                    <div id="sale_order_communication" class="mt-4">
                        <h2>Histórico</h2>
                        <t t-call="portal.message_thread">
                            <t t-set="object" t-value="nr_equipamento" />
                        </t>
                    </div>
                </section>
            </t>
        </xpath>
    </template>
